<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="fragment_onboard_facilityManagementApp_tv_style">
        <item name="android:textColor">@color/black_400</item>
        <item name="android:textSize">@dimen/fragment_onboard_facilityManagementApp_textSize</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="fragment_onboard_manageComplaint_tv_style">
        <item name="android:textColor">#BF010F07</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="fragment_singleComplaint_tv_style">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="CustomTextAppearanceTab" parent="TextAppearance.Design.Tab">
        <item name="android:textColor" >@color/green</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>

    </style>
    <style name="profile_format">
        <item name="android:fontFamily">@font/sfprotext</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:lineSpacingExtra">3dp</item>
    </style>
    <style name="profile_details">
        <item name="android:fontFamily">@font/sfprotext</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/gray3</item>
    </style>

    <style name="MyDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorOnPrimary">@color/black</item>
    </style>

</resources>